<ul>
	<li>
		<a href="/">Home</a>
	</li>
	<% if (userSession) { %>
		<li>
			<a href="/profile/<%= userSession.id %>">
				<%= userSession.username %>
			</a>
			<a href="/logout">Logout</a>
		</li>
	<% } else { %>
		<li>
			<a href="/login">Login</a>
		</li>
		<li>
			<a href="/signup">Sign Up</a>
		</li>
	<% } %>
</ul>
